<table class="kMatrix">
<caption></caption>

<tr>
   {{each(idx,column) columns}}
            <th class="ui-widget-header">${column.name}</td>
   {{/each}}
</tr>

{{each(idx,row) values}}
<tr data-id="${$item.getIndex(row,pk)}">
   {{each(colIdx,column) columns}}
		<td class="ui-widget-content{{if (column.default)}} default{{/if}}">{{html $item.getIndexFromColumn(row,column)}}</td>
	{{/each}}
</tr>
{{/each}}
</table>