<span><input type="checkbox" rel="${_elemName}" /><?php echo $this->translate('Change password'); ?><br /></span>
<input
    {{if (_properties.required)}}
        required=""
    {{/if}}
    type="password"
    class="password"
    name="${_elemName}"
    id="${_elemIden}"
    value="${_fieldValue}"
    {{each(idx,attrValue) _dataConfig.attributes}}
        ${idx}="${attrValue}"
    {{/each}}
/>
