<input
    type="checkbox"
    rel="${_elemName}"

    {{if (_fieldValue && _fieldValue!='0')}}
        checked="checked"
    {{/if}}

    {{each(idx,attrValue) _dataConfig.attributes}}
        ${idx}="${attrValue}"
    {{/each}}

/>
<input
    type="hidden"
    id="${_elemIden}"
    name="${_elemName}"
    value="${_fieldValue}"
    class="checkbox"
/>
