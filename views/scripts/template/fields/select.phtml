<select name="${_elemName}" id="${_elemIden}" {{if (_readonly)}} disabled="disabled"{{/if}}>
{{each(idx,dataValue) _dataConfig}}
    {{if (idx != '__className')}}
        <option{{if (idx == _fieldValue)}} selected="selected"{{/if}} value="${idx}">${dataValue}</option>
    {{/if}}
{{/each}}
</select>