{{if (_readonly)}}
    {{html _dataConfig[_fieldValue]}}
    <input type="hidden" name="${_elemName}" value="${_fieldValue}" />
{{else}}
<select name="${_elemName}" id="${_elemIden}">
{{each(idx,dataValue) _dataConfig}}
    {{if (idx != '__className')}}
        <option{{if (_fieldValue && idx == _fieldValue)}} selected="selected"{{/if}} value="${idx}">${dataValue}</option>
    {{/if}}
{{/each}}
</select>
{{/if}}